~inc:footer.inc~
<script type="text/javascript">

function updateStatus(xmlData) {

	if(!xmlData)
	{
		return;
	}

	for(i = 0; i <= 7; i++) 
    {
		if( i != 5)
		{
			switch(getXMLValue(xmlData,'BUC1', 'led'+i))
			{
				case '0': document.getElementById('led' + i).style.color = '#FF0000'; break;
				case '1': document.getElementById('led' + i).style.color = '#00FF00'; break;
				case '2': document.getElementById('led' + i).style.color = '#FFA500'; break;
				case '3': document.getElementById('led' + i).style.color = '#A8A8A8'; break;
				default: document.getElementById('led' + i).style.color = '#ffb4f5'; break;
			}
			
			switch(getXMLValue(xmlData,'BUC2', 'led'+i))
			{
				case '0': document.getElementById('ned' + i).style.color = '#FF0000'; break;
				case '1': document.getElementById('ned' + i).style.color = '#00FF00'; break;
				case '2': document.getElementById('ned' + i).style.color = '#FFA500'; break;
				case '3': document.getElementById('ned' + i).style.color = '#A8A8A8'; break;
				default: document.getElementById('ned' + i).style.color = '#ffb4f5'; break;
			}
		}
	}
	switch(getXMLValue(xmlData,'rcu', 'pathAAlarm'))
	{
		case '0': document.getElementById('pathAAlarm').style.color = '#FF0000'; break;
		case '1': document.getElementById('pathAAlarm').style.color = '#00FF00'; break;
		case '2': document.getElementById('pathAAlarm').style.color = '#FFA500'; break;
		case '3': document.getElementById('pathAAlarm').style.color = '#A8A8A8'; break;
	}
	switch(getXMLValue(xmlData, 'rcu', 'pathBAlarm'))
	{
		case '0': document.getElementById('pathBAlarm').style.color = '#FF0000'; break;
		case '1': document.getElementById('pathBAlarm').style.color = '#00FF00'; break;
		case '2': document.getElementById('pathBAlarm').style.color = '#FFA500'; break;
		case '3': document.getElementById('pathBAlarm').style.color = '#A8A8A8'; break;
	}
	switch(getXMLValue(xmlData, 'rcu', 'PS1alarm'))
	{
		case '0': document.getElementById('PS1alarm').style.color = '#FF0000'; break;
		case '1': document.getElementById('PS1alarm').style.color = '#00FF00'; break;
		case '2': document.getElementById('PS1alarm').style.color = '#FFA500'; break;
		case '3': document.getElementById('PS1alarm').style.color = '#A8A8A8'; break;
	}
	switch(getXMLValue(xmlData, 'rcu', 'PS2alarm'))
	{
		case '0': document.getElementById('PS2alarm').style.color = '#FF0000'; break;
		case '1': document.getElementById('PS2alarm').style.color = '#00FF00'; break;
		case '2': document.getElementById('PS2alarm').style.color = '#FFA500'; break;
		case '3': document.getElementById('PS2alarm').style.color = '#A8A8A8'; break;
	}	
	switch(getXMLValue(xmlData, 'rcu', 'OCXO1alarm'))
	{
		case '0': document.getElementById('OCXO1alarm').style.color = '#FF0000'; break;
		case '1': document.getElementById('OCXO1alarm').style.color = '#00FF00'; break;
		case '2': document.getElementById('OCXO1alarm').style.color = '#FFA500'; break;
		case '3': document.getElementById('OCXO1alarm').style.color = '#A8A8A8'; break;
	}
	switch(getXMLValue(xmlData, 'rcu', 'OCXO2alarm'))
	{
		case '0': document.getElementById('OCXO2alarm').style.color = '#FF0000'; break;
		case '1': document.getElementById('OCXO2alarm').style.color = '#00FF00'; break;
		case '2': document.getElementById('OCXO2alarm').style.color = '#FFA500'; break;
		case '3': document.getElementById('OCXO2alarm').style.color = '#A8A8A8'; break;
	}
	switch(getXMLValue(xmlData, 'rcu', 'buc1comstat'))
	{
		case '0': document.getElementById('buc1comstat').style.color = '#FF0000'; break;
		case '1': document.getElementById('buc1comstat').style.color = '#00FF00'; break;
		case '2': document.getElementById('buc1comstat').style.color = '#FFA500'; break;
		case '3': document.getElementById('buc1comstat').style.color = '#A8A8A8'; break;
	}
	switch(getXMLValue(xmlData, 'rcu', 'buc2comstat'))
	{
		case '0': document.getElementById('buc2comstat').style.color = '#FF0000'; break;
		case '1': document.getElementById('buc2comstat').style.color = '#00FF00'; break;
		case '2': document.getElementById('buc2comstat').style.color = '#FFA500'; break;
		case '3': document.getElementById('buc2comstat').style.color = '#A8A8A8'; break;
	}
}

newAJAXCommand('status.xml', updateStatus, true);

</script>
	</body>
</html>
